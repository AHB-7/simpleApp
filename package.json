{
    "dependencies": {
        "bootstrap": "^5.3.3"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.4",
        "eslint": "^8.57.0",
        "jest": "^29.2.0",
        "prettier": "^3.2.5",
        "sass": "^1.72.0"
    },
    "scripts": {
        "test": "npm run test-unit",
        "test-unit": "jest",
        "build": "sass src/scss:dist/css",
        "watch": "sass --watch src/scss:dist/css",
        "lint": "eslint **/*.js",
        "format": "prettier --write **/*.js",
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ]
        ]
    }
}
